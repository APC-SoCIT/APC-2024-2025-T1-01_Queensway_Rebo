{
  "name": "Laravel Dev",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "forwardPorts": [8000, 3306],
  "postCreateCommand": "cp .env.example .env && composer install && php artisan key:generate",
  "customizations": {
    "vscode": {
      "extensions": [
        "xdebug.php-debug",
        "bmewburn.vscode-intelephense-client"
      ]
    }
  },
  "mounts": [
    "source=${localWorkspaceFolder}/ecommerce.sql,target=/workspace/ecommerce.sql,type=bind"
  ]
}
