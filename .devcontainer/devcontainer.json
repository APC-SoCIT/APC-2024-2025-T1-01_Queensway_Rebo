{
  "name": "Laravel + MySQL",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/var/www/html",
  "postCreateCommand": "cp .env.example .env && composer install && php artisan key:generate",
  "forwardPorts": [8000],
  "customizations": {
    "vscode": {
      "extensions": [
        "bmewburn.vscode-intelephense-client",
        "xdebug.php-debug"
      ]
    }
  }
}
