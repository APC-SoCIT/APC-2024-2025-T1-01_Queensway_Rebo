{
  "name": "Laravel + MySQL",
  "dockerComposeFile": "docker-compose.yml",
  "service": "laravel",
  "workspaceFolder": "/workspace",
  "forwardPorts": [8000],
  "postCreateCommand": "cp .env.example .env && composer install && php artisan key:generate && ./init-db.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "bmewburn.vscode-intelephense-client",
        "xdebug.php-debug"
      ]
    }
  }
}
